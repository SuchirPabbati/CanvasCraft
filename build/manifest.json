{
    "manifest_version": 3,
    "name": "PDFTab",
    "description": "Open PDFs in a new tab",
    "version": "1.0",
    "icons": {
        "16": "images/icon16.png",
        "128": "images/icon128.png"
    },
    "permissions": ["tabs", "activeTab"],
    
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["contentscript.js"]
    }]
}